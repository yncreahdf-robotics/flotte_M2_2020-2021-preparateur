<launch>
  
   <node pkg="rosserial_python" type="serial_node.py" name="ard1" output="screen">
      <param name="port" value="/dev/ttyACM1"/>
      <param name="baud" value="57600"/>
   </node>

   <node pkg="rosserial_python" type="serial_node.py" name="ard2" output="screen">
      <param name="port" value="/dev/ttyACM0"/>
      <param name="baud" value="57600"/>
   </node>

   <node pkg="rosserial_python" type="serial_node.py" name="ard3" output="screen">
      <param name="port" value="/dev/ttyACM2"/>
      <param name="baud" value="57600"/>
   </node>

   <node pkg="rosserial_python" type="serial_node.py" name="ard4" output="screen">
      <param name="port" value="/dev/ttyACM3"/>
      <param name="baud" value="57600"/>
   </node>

   <node pkg="rosserial_python" type="serial_node.py" name="ard5" output="screen">
      <param name="port" value="/dev/ttyACM4"/>
      <param name="baud" value="57600"/>
   </node>

   <node pkg="rosserial_python" type="serial_node.py" name="ard6" output="screen">
      <param name="port" value="/dev/ttyUSB1"/>
      <param name="baud" value="57600"/>
   </node>

